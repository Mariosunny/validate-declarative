{
  "name": "validate-declarative",
  "version": "0.0.0",
  "description": "A simple utility for declaratively validating any Javascript object.",
  "main": "",
  "scripts": {
    "prepublish": "yarn test && yarn build",
    "build": "webpack src/index.js -p --target=node --output-library-target commonjs2",
    "benchmark": "babel-watch stats/benchmarks.js",
    "size": "yarn build && du -s -B1 --apparent-size dist/main.js | awk '{print $1}'",
    "test": "jest",
    "test:verbose": "jest --verbose",
    "start": "babel-watch main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mariosunny/validate-declarative.git"
  },
  "author": "Tyler Hurson <tyler.james.hurson@gmail.com>",
  "license": "ISC",
  "dependencies": {
  },
  "keywords": [
    "validation",
    "validator",
    "validate",
    "valid",
    "declarative",
    "object",
    "assert",
    "assertion",
    "schema",
    "parse",
    "sanitize",
    "sanitise"
  ],
  "files": [
    "dist/index.js"
  ],
  "devDependencies": {
    "deep-strict-equal": "^0.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-watch": "^2.0.7",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "outliers": "^0.0.3",
    "prettier": "^1.14.3",
    "unravel-function": "^0.2.1",
    "webpack": "^4.23.0",
    "webpack-cli": "^3.1.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "bail": true,
    "collectCoverage": true
  }
}
