{
  "name": "validate-declarative",
  "version": "0.1.3",
  "description": "A simple, highly-extensible utility for validating the structure of any JS object.",
  "main": "",
  "scripts": {
    "prepublish": "npm run test && npm run build",
    "build": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "benchmark": "babel-watch stats/benchmarks.js",
    "size": "npm run build && du -s -B1 --apparent-size dist/index.js | awk '{print $1}'",
    "test": "jest",
    "test:verbose": "jest --verbose",
    "start": "babel-watch main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mariosunny/validate-declarative.git"
  },
  "author": "Tyler Hurson <tyler.james.hurson@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "deep-strict-equal": "^0.2.0"
  },
  "keywords": [],
  "files": [
    "dist/index.js"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-watch": "^2.0.7",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "outliers": "^0.0.3",
    "prettier": "^1.14.3",
    "unravel-function": "^0.2.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
